{
    "manifest_version": 2,
    "name": "niconico Darkmode",
    "version": "0.2.0.0",
    "description": "niconicoサイトをダークモード化する拡張機能",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png",
        "500": "images/icon500.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.nicovideo.jp/",
                "https://www.nicovideo.jp/?*"
            ],
            "js": [
                "js/jquery-3.5.1.min.js","js/main.js"
            ],
            "css": [
                "top/social-top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/daily_topics/*"
            ],
            "js": [
                "js/jquery-3.5.1.min.js","js/main.js"
            ],
            "css": [
                "top/daily_topics.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/recommendnews",
                "https://www.nicovideo.jp/recommendnews*"
            ],
            "css": [
                "video/parts/zero_design.css",
                "top/recommendnews.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_top*",
                "https://www.nicovideo.jp/video_top?*",
                "https://www.nicovideo.jp/video_top/genre*"
            ],
            "css": [
                "video/parts/cre_design.css",
                "video/video_top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/watch/*"
            ],
            "css": [
                "video/watch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/search/*",
                "https://www.nicovideo.jp/tag/*",
                "https://www.nicovideo.jp/mylist_search/*",
                "https://www.nicovideo.jp/newarrival*",
                "https://www.nicovideo.jp/recent*",
                "https://www.nicovideo.jp/related_tag*"
            ],
            "css": [
                "video/parts/zero_design.css",
                "video/serch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/rss*"
            ],
            "css": [
                "top/rss.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/my*",
                "https://www.nicovideo.jp/user/*"
            ],
            "css": [
                "video/mypage/re.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/ranking*"
            ],
            "css": [
                "video/parts/cre_design.css",
                "video/ranking.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/recommendations*"
            ],
            "css": [
                "video/parts/cre_design.css",
                "video/recommendations.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/openlist*",
                "https://www.nicovideo.jp/mymemory_register"
            ],
            "css": [
                "video/parts/zero_design.css",
                "video/recommendations.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/ranking_graph*"
            ],
            "css": [
                "video/parts/zero_design.css",
                "video/ranking_graph.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_catalog*"
            ],
            "css": [
                "video/parts/zero_design.css",
                "video/other.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/",
                "https://live.nicovideo.jp/?*",
                "https://live2.nicovideo.jp/",
                "https://live2.nicovideo.jp/?*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/live_top.css"
            ]
        },
        {
            "matches": [
                "https://live2.nicovideo.jp/focus",
                "https://live2.nicovideo.jp/focus?*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/forcus.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/recent",
                "https://live.nicovideo.jp/recent?*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/recent.css"
            ]
        },
        {
            "matches": [
                "https://site.live.nicovideo.jp/recent/namagame*"
            ],
            "css": [
                "live/site-live/namagame.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/timetable*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/timetable.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/ranking*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/ranking.css"
            ]
        },
        {
            "matches": [
                "https://live2.nicovideo.jp/watch/*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/watch.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/follow*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/parts/card.css",
                "live/follow.css"
            ]
        },{
            "matches": [
                "https://live.nicovideo.jp/my*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/my.css"
            ]
        },{
            "matches": [
                "https://live2.nicovideo.jp/create*"
            ],
            "css": [
                "live/create.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/search*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/search.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/history*"
            ],
            "css": [
                "live/parts/live_parts.css",
                "live/history.css"
            ]
        },
        {
            "matches": [
                "https://account.nicovideo.jp/login*"
            ],
            "css": [
                "account/login.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/feedback/*"
            ],
            "css": [
                "top/feedback.css"
            ]
        }
    ],
    "permissions": [
        "storage"
      ]
}